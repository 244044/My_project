/* pages/cart/cart.wxss */
page{
    background-color: #f5f6f7;
}
.goodslist{
    display: flex;
    padding: 20rpx 0px;
    background-color: #fff;
    margin-bottom: 2rpx;
}
.goodslist .left{
    width: 240rpx;
    text-align: center;
}
.goodslist .left image{
    width: 200rpx;
    height: 200rpx;

}
.goodslist .right{
    flex:1;
    height: 200rpx;
    position: relative;
    margin-right: 20rpx;
}
.goodslist .right image{
    width: 44rpx;
    height: 44rpx;
}
.goodslist .right .goods_name{
    font-size: 36rpx;
    color: #808080;
    height: 100rpx;
    overflow: hidden;
}
.goodslist .right .goods_price{
    font-size: 25rpx;
    color: #ff002b;
}
.goodslist .right .car{
    position: absolute;
    right: 0px;
    bottom: 0px;
}
.goodslist .carBtn{
    width: 200rpx;
    position: absolute;
    right: 100rpx;
    bottom: 0rpx;
}
.goodslist .carBtn .btn{
    width: 52rpx;
    height: 52rpx;
    text-align: center;
    line-height: 52rpx;
    font-size: 24rpx;
    float: left;
    border:1rpx solid #b2b2b2;

}
.goodslist .carBtn .num{
    float: left;
    text-align: center;
    width: 80rpx;
    height: 52rpx;
    line-height: 52rpx;
    border-top: 1px solid #b2b2b2;
    border-bottom: 1px solid #b2b2b2;
}
.bottomNav{
    position: fixed;
    left: 0px;
    bottom: 0px;
    background-color: #fff;
    width: 100%;
    height: 100rpx;
    border-bottom: 1rpx solid #f5f6f7;
    font-size: 28rpx;

}
.bottomNav .warp{
    padding: 0 20rpx;
    line-height: 100rpx;
}
.bottomNav .warp text{
    color: #ff002b;
}
.bottomNav .warp .order{
    float: right;
    height: 60rpx;
    line-height: 60rpx;
    width: 160rpx;
    color: #fff;
    background-color: #246f6f;
    text-align: center;
    margin-top: 20rpx;
    border-radius: 2rpx;
}
.goodsUnll{
    text-align: center;
    margin: 30rpx 0;

}
.goodsUnll image{
    width: 140rpx;
    height: 140rpx;

}
.goodsUnll view{
    font-size: 40rpx;
    columns: #808080;
    margin-top: 30rpx;
}