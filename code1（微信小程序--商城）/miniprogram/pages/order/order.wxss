page{
   background-color: #f5f5f5;
}
.swiperTab{
   width: 100%;
   height: 100rpx;
   position: fixed;
   top: 0;
   left: 0;
   z-index: 999;
}
.swiperTab view{
   float:left;
   width: 33.3%;
   text-align: center;
   line-height: 100rpx;
   background-color: #f5f5f5;
   border-bottom: 2rpx solid #f5f5f5;
}
.swiperTab .con{
   border-bottom: 2rpx solid #246f6f;
   color: #246f6f;
}

/*  */
.swiperContent{
   margin-top: 140rpx;
}
.item{
   width: 90%;
   margin: 0 auto 40rpx;
   background-color: #fff;
   border-radius:20rpx;
}
.item .goods{
   width: 90%;
   margin: 0 auto;
   padding: 40rpx 0;
   overflow: hidden;
}
.item .goods image{
   width: 20%;
   float: left;
}
.item .goods .middle{
   float: left;
   width: 55%;
}
.item .goods .middle .title{
   font-size: 36rpx;
   height: 60rpx;
   overflow: hidden;
   white-space: nowrap;
   text-overflow: ellipsis;
   padding-left: 20rpx;
}
.item .goods .middle .other{
   margin-left: 20rpx;
   font-size: 24rpx;
   color: #ff002b;
   background-color: #FAF2E9;
   border-radius: 10rpx;
}
.item .goods .right{
   float: right;
   text-align: right;
   width: 25%;
}
.item .goods .right .num{
   margin: 12rpx 0;
   color: #727272;
}
.item .total{
   width: 90%;
   margin: 0 auto;
   overflow: hidden;
}
.item .total .totalPrice{
   float: right;
}
.item .total .totalPrice view{
   float: left;
}
.item .total .totalPrice .left{
   color: #727272;
   margin-right: 16rpx;
}
.item .down{
   width: 90%;
   margin: 0 auto;
   overflow: hidden;
   padding: 40rpx 0;
}
.item .down .btn{
   float: right;
   border: 2rpx solid #246f6f;
   color: #246f6f;
   padding: 12rpx 40rpx;
   border-radius: 80rpx;
}